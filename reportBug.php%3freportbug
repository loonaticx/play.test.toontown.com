var logs=new Array();
var logsi=0;

function logit(){
    var a ="";

    for (var i=0; i<arguments.length; i++) {
      if (i%2 == 0) {
        // Name
        a+=escape(arguments[i]);
        a+= "=";
      } else {
        // Value
        a+=escape(arguments[i]);
        a+=((i+1)!=arguments.length)?"&":"";
      }
    
    }

  var s="./shared/www/collect.php?rand="+Math.random()+"&"+a;
  var lognum = logsi++;
  logs[lognum%9]=new Image();
  logs[lognum%9].src=s;
}

