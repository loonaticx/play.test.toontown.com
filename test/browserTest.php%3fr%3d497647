  }

  var br=new Array(4);
  var os=new Array(2);
  br=getBrowser();
  os=getOS();
  var appleWebKit = -1;//unknown

  if ( (navigator.platform == 'MacIntel') || (navigator.platform == 'MacPPC') ){
    var appleWebKit = getAppleWebKit(); 	
