    function br_params(qp,dflt){
    dflt=(dflt==null)?-1:dflt;
    try{
      r=unescape(navigator.userAgent.match(new RegExp(qp+"( |\:|\/)+([^&;\) ]*)"))[2]);
    } catch(qp) { r=dflt; }
    return r;
  }

  msieBool = br_params("MSIE",0);
  operaBool = br_params("Opera",0);
  intelMac = br_params("intelMac",0);
  firefox = br_params("Firefox",0);
  netscapeBool = br_params("Netscape",0);
  safariDetection = br_params("Safari",0);


  if(msieBool){
    browser = "msie"
    strippedAppVersion=br_params("MSIE");
  }
  else if(operaBool){
    browser = "opera";
    strippedAppVersion= br_params("Opera");
  }
  else if(firefox){
    browser = "Firefox";
    strippedAppVersion= br_params("Firefox");
  }
  else if(netscapeBool){
    strippedAppVersion = br_params("Netscape");
    browser = "Netscape";
  }
  else if(safariDetection){
    browser = "Safari";
    strippedAppVersion = br_params("Safari");
  }


  document.write('<form action="browserTestResult.php" method="post" name="webclientspecs" >');
  document.write('<input type="hidden" name="platform" value="'+ navigator.platform +'" >');
  document.write('<input type="hidden" name="browser" value="'+ browser +'" >');
  document.write('<input type="hidden" name="browserVersion" value="'+ strippedAppVersion+'" >');
