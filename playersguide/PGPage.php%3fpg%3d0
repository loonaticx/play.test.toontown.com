function pgOpenCloseSections(){
  document.getElementById("pgOpenCloseTopTxt").innerHTML='';
  document.getElementById("pgOpenCloseBotTxt").innerHTML='';
  if (pgSectionToggle==0){
    for (i=1; i <= 20;i++){
      tempTocId='';
      tempTocId='toc_section_'+i;
      tempSection=document.getElementById(tempTocId);
      tempSection.style.display = "block";
      tempImageId='toc_sect_img_'+i;
      sect_img = document.getElementById(tempImageId);
      sect_img.src="./playersguide/images/arrow_down.gif";
    }
    pgSectionToggle=1;//toggle between close and open section links
    document.getElementById("pgOpenCloseTopTxt").innerHTML='Collapse all';
    document.getElementById("pgOpenCloseBotTxt").innerHTML='Collapse all';
  }
  else {
    for (i=1; i <= 20;i++){
      tempTocId='';
      tempTocId='toc_section_'+i;
      tempSection=document.getElementById(tempTocId);
      tempSection.style.display = "none";
      tempImageId='toc_sect_img_'+i;
      sect_img = document.getElementById(tempImageId);
      sect_img.src="./playersguide/images/arrow_right.gif";
    }
    pgSectionToggle=0;//toggle between close and open section links
    document.getElementById("pgOpenCloseTopTxt").innerHTML='Expand all';
    document.getElementById("pgOpenCloseBotTxt").innerHTML='Expand all';
  }
}
-->
</script>
