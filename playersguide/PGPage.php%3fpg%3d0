<script type="text/javascript">
<!--
if (!document.getElementById) document.getElementById = function() { return null; }

//toggle for Expand and Collapse link
pgSectionToggle=0;//section links closed

function checkAllSections(){
// Check if all sections are closed or open
// if all sections are closed, updated pgOpenCloseSections()
// if all sections are open, updated pgOpenCloseSections()
  var SectionBlocks=0;
  var SectionNone=0;

  for (i=1; i <= 20;i++){
    tempTocId='';
    tempTocId='toc_section_'+i;
    tempSection=document.getElementById(tempTocId);
    if (tempSection.style.display == "block"){
      SectionBlocks++;
    }
    else if (tempSection.style.display == "none"){
      SectionNone++;
    }
  }

  if (SectionBlocks == 20){
    //all sections are closed
    pgSectionToggle=0;//toggle between close and open section links
    pgOpenCloseSections();
  } else if (SectionNone == 20){
    //all sections are open
    pgSectionToggle=1;//toggle between close and open section links
    pgOpenCloseSections();
  }
  //alert('block: '+SectionBlocks+"\n"+'none: '+SectionNone);
}

function initTocItem(tocId, actuatorId, imageId) {
  var section = document.getElementById(tocId);
  var actuator = document.getElementById(actuatorId);
  var sect_img = document.getElementById(imageId);
  section.style.display = "none";

  if (section == null || actuator == null) return;

  actuator.onclick = function() {
    var display = section.style.display;
    if (display == "block") {
      section.style.display = "none";
      sect_img.src="./playersguide/images/arrow_right.gif";
    } else {
      section.style.display = "block";
      sect_img.src="./playersguide/images/arrow_down.gif";
    }
    checkAllSections();
    return false;
  }
}

window.onload = function() {
  initTocItem("toc_section_1", "toc_link_1", "toc_sect_img_1");
  initTocItem("toc_section_2", "toc_link_2", "toc_sect_img_2");
  initTocItem("toc_section_3", "toc_link_3", "toc_sect_img_3");
  initTocItem("toc_section_4", "toc_link_4", "toc_sect_img_4");
